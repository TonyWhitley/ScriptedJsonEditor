{
  "# Config file for ScriptedJsonEditor": "",
  "# V1.0.0": "",
  "# 'jobs' file to set up a G25 for use with a monitor": "",
  "<PLAYER.JSON>": "<RF2ROOT>\\UserData\\<PLAYER>\\<PLAYER>.JSON",
  "<CONTROLLER.JSON>": "<RF2ROOT>\\UserData\\<PLAYER>\\Controller.JSON",
  "skip keys with # in them": true,
  "# keys with # in them are used as comments, don't change the values": "",
  "rFactor escape slash": true,
  "# rFactor 2 escapes /. Also remove space after the :": "",
  "jobs file format": 6,
  "job definition files": [
    "job_definitions\\Keyboard_jobs.json",
    "job_definitions\\G25_jobs.json"
  ],
  "jobs": [
    {
      "Keyboard_jobs": [
        "Driver aid buttons disable",
        "Cursor keys control seat"
      ],
      "G25_jobs": [
        "Wheel settings",
        "Monitor",
        "POV_hat_LCD_control",
        "Minor controls"
      ]
    }
  ]
}